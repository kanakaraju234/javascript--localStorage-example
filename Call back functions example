<script>
    
function myBigfunction(entervalue)
    {
        var myValue =entervalue;
        
        subFunction1(myValue);
        subFunction2(myValue);
        subFunction3(myValue);
    }
    
function subFunction1(value)
    {
       console.log(value++);
    }
function subFunction2(value)
    {
       console.log(value+1);
    }
function subFunction3(value)
    {
       console.log(value+2);
    }
    
</script>

